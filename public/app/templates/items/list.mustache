
<div class="page-header">
	<h1>
		Items List
	</h1>
</div>
<div class="row">
	<div class="span4">
		<table class="table table-striped table-bordered">
		  <thead>
		    <tr>
		      <th>Item</th>
		      <th>Price</th>
		    <tr>
		  </thead>
		  <tbody>
		    {{#items}}
		      <tr>
		        <td>{{name}}</td>
		        <td>{{price}}</td>
		      </tr>
		    {{/items}}
		  </tbody>
		</table>
	</div>
</div>
