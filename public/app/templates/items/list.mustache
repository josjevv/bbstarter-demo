
<div class="page-header">
	<h1>
		Items List
	</h1>
</div>
<div class="row">
	<div class="span16">
		<ul>
	      {{#items}}
		   <li>{{name}} - {{price}}</li>
		  {{/items}}
		</ul>
	</div>
  </div>
</div>
<div class="row">
    <div class="span3">
    </div>
    <div class="span9">
      <form class="well">
        <label>Add New Item</label>
        <input type="text" class="span3" placeholder="Item name"> <span class="help-inline">Associated help text!</span>
        <label class="checkbox">
          <input type="checkbox"> Check me out
        </label>
        <button type="submit" class="btn add">Add</button>
      </form>
    </div>
</div>	
